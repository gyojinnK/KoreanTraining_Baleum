{
    "compilerOptions": {
      "jsx": "react",
      "lib": ["es6", "dom"],
      "rootDir": "src",
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "es5",
      "sourceMap": true,
      "moduleResolution": "node",
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "resolveJsonModule": true,
    },
    "exclude": [
      "node_modules",
      "build",
      "**/build.gradle",
    ],
    "include": [
      "./src"
    ],
  }